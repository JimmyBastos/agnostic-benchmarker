{
  "name": "angular-benchmark",
  "version": "1.0.0",
  "private": true,
  "framework-metadata": {
    "frameworkVersionFromPackage": "@angular/core"
  },
  "scripts": {
    "ng": "cross-env ng",
    "start:dev": "cross-env ng serve --host 0.0.0.0 --port 8080",
    "start:server": "cross-env http-server ./dist",
    "start:prod": "cross-env npm run build:prod && npm run start:server",
    "build:prod": "cross-env ng build --prod --stats-json",
    "analize": "cross-env npm run build:prod && webpack-bundle-analyzer ./dist/stats.json",
    "lint": "cross-env ng lint"
  },
  "browserslist": [
    "> 0.5%",
    "last 2 versions",
    "Firefox ESR",
    "not dead",
    "not IE 9-11"
  ],
  "dependencies": {
    "@angular/common": "~7.1.0",
    "@angular/compiler": "~7.1.0",
    "@angular/core": "~7.1.0",
    "@angular/platform-browser": "~7.1.0",
    "@angular/platform-browser-dynamic": "~7.1.0",
    "@types/lodash": "^4.14.119",
    "core-js": "^2.5.4",
    "pure-css": "^1.0.4",
    "rxjs": "~6.3.3",
    "tslib": "^1.9.0",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.11.0",
    "@angular/cli": "~7.1.3",
    "@angular/compiler-cli": "~7.1.0",
    "@angular/language-service": "~7.1.0",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.5.0",
    "cross-env": "^5.2.0",
    "http-server": "^0.11.1",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~3.1.6"
  }
}

{
  "name": "vue-ts",
  "version": "0.1.0",
  "private": true,
  "framework-metadata": {
    "frameworkVersionFromPackage": "vue"
  },
  "scripts": {
    "start:dev": "cross-env vue-cli-service serve",
    "start:server": "cross-env http-server ./dist",
    "start:prod": "cross-env npm run build:prod && npm run start:server",
    "build:prod": "cross-env vue-cli-service build --report-json --mode production",
    "postbuild:prod": "cross-env postcss ./dist/**/*.css --replace",
    "analize": "cross-env npm run build:prod && webpack-bundle-analyzer ./dist/report.json"
  },
  "dependencies": {
    "@types/lodash": "^4.14.119",
    "pure-css": "^1.0.4",
    "register-service-worker": "^1.5.2",
    "vue": "^2.5.21",
    "vue-class-component": "^6.0.0",
    "vue-property-decorator": "^7.0.0"
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^1.1.0",
    "@vue/cli-plugin-babel": "^3.1.1",
    "@vue/cli-plugin-pwa": "^3.1.1",
    "@vue/cli-plugin-typescript": "^3.1.1",
    "@vue/cli-service": "^3.1.1",
    "cross-env": "^5.2.0",
    "http-server": "^0.11.1",
    "node-sass": "^4.9.0",
    "postcss-cli": "^6.1.1",
    "sass-loader": "^7.0.1",
    "typescript": "^3.0.0",
    "vue-template-compiler": "^2.5.21"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}

{
  "name": "vue-bechmark",
  "version": "1.0.0",
  "private": true,
  "framework-metadata": {
    "frameworkVersionFromPackage": "vue"
  },
  "scripts": {
    "start:dev": "cross-env vue-cli-service serve",
    "start:server": "cross-env http-server ./dist",
    "start:prod": "cross-env npm run build:prod && npm run start:server",
    "build:prod": "cross-env vue-cli-service build --report-json --mode production",
    "postbuild:prod": "cross-env postcss ./dist/**/*.css --replace",
    "analize": "cross-env npm run build:prod && webpack-bundle-analyzer ./dist/report.json"
  },
  "browserslist": [
    "> 0.5%",
    "last 2 versions",
    "Firefox ESR",
    "not dead",
    "not IE 9-11"
  ],
  "dependencies": {
    "lodash": "^4.17.11",
    "pure-css": "^1.0.4",
    "vue": "^2.5.17"
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^1.1.0",
    "@vue/cli-plugin-babel": "^3.1.1",
    "@vue/cli-plugin-eslint": "^3.1.1",
    "@vue/cli-service": "^3.1.1",
    "@vue/eslint-config-standard": "^4.0.0",
    "babel-eslint": "^10.0.1",
    "cross-env": "^5.2.0",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0-0",
    "http-server": "^0.11.1",
    "node-sass": "^4.9.0",
    "postcss-cli": "^6.1.1",
    "sass-loader": "^7.0.1",
    "vue-template-compiler": "^2.5.17"
  }
}
